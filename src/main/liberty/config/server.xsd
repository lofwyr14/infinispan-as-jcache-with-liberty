                <xsd:appinfo> 
                    <ext:label>Prüfdatensätze im Prüfdateiprotokoll verschlüsseln</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="encryptAlias" use="optional" type="xsd:string"> 
