                <xsd:annotation> 
                    <xsd:documentation>Eine Gruppenzugriffs-ID im allgemeinen Format Gruppe:Realmname/eindeutige_ID_der_Gruppe. </xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Gruppenzugriffs-ID</ext:label> 
