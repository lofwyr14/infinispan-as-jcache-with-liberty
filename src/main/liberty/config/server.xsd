                        </xsd:enumeration> 
                        <xsd:enumeration value="ITTDistinguishedName"> 
                            <xsd:annotation> 
                                <xsd:documentation>ITTDistinguishedName-Identitätstoken zusichern</xsd:documentation> 
                            </xsd:annotation> 
                        </xsd:enumeration> 
                    </xsd:restriction> 
                </xsd:simpleType> 
            </xsd:element> 
