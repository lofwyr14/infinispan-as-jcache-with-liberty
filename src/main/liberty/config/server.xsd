                <xsd:appinfo> 
                    <ext:label>Größe des Prüfdateiprotokolls</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxFiles" use="optional" default="100" type="intType"> 
