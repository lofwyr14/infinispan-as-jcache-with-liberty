                <xsd:documentation>Gibt den Namen der Persistenzeigenschaft auf Integrationsebene an.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Name der Persistenzeigenschaft</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
