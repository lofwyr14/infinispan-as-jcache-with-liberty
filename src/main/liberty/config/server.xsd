                        <ext:label>JCache-Implementierungsbibliothek</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="commonLibrary" type="com.ibm.ws.classloading.sharedlibrary-factory"> 
