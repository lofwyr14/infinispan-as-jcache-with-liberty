                        <ext:label>Referenz des Keystores für Prüfverschlüsselung</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="signingKeyStore" type="com.ibm.ws.ssl.keystore"> 
                <xsd:annotation> 
