    <xsd:complexType name="com.ibm.ws.jca.connectionFactory"> 
        <xsd:annotation> 
            <xsd:documentation>Definiert eine Verbindungsfactory-Konfiguration. </xsd:documentation> 
            <xsd:appinfo> 
